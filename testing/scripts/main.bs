//Load Interface for IO
fs = environment.loadInterface("fs", {})

//Install Module System
moduleInstallerSource = fs.readAll("modules_install.bs")
moduleInstaller = environment.loadString(moduleInstallerSource)
moduleInstaller.load()

//Load Modules System
modulesSource = fs.readAll("modules.bs")
__G.modules = environment.loadString(modulesSource)

//Require the interactive shell module
shell = modules.require("interactive-shell")


//Use Interactive Shell Module
shell.runShell()