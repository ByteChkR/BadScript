set USER $(whoami)
set HOME_DIR $(combine "/home/" %USER%)
set STARTUP_FILE $(combine %HOME_DIR% "/.BadShell/startup.bsh")

cd %HOME_DIR%

if $(eq $(exists "./.BadShell") false) $({
	mkdir "./.BadShell"
})

if $(isfile %STARTUP_FILE%) $({
	$[STARTUP_FILE]
})

echo $(loadtext "/data/BadShell/login_info.txt")